<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Harbor Documentation"
        sling:resourceType="granite/ui/components/foundation/page"
        sling:vanityOrder="{Long}301"
        sling:vanityPath="">

        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/meta"
                content="chrome=1"
                name="X-UA-Compatible"/>
            <winmode
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/page/winmode"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/page/title"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                categories="[coralui2,granite.ui.foundation,granite.ui.foundation.admin,cq.common.wcm,cq.siteadmin.admin,cq.listview.columns.personalization,cq.listview.analytics,cq.siteadmin.admin.pagecard]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/page/body">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/layouts/page"/>
            <rail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/navcolumns"
                currentId="harbor-admin-documentation-components"
                srcPath="cq/core/content/nav"/>
            <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/breadcrumbs"
                baseUrl=""
                contentPath="/libs/harbor/admin/documentation/components"
                depthAdjustment="{Long}-1"
                pageTitleTemplate="{{title}}"
                rootPageTitle="Harbor"
                rootTitle="Documentation"
                src="/{+id}"
                target=".cq-siteadmin-admin-childpages"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/title"
                contentPath="/libs/harbor/admin/documentation/components"
                rootTitle="Harbor"/>
            <header
                jcr:primaryType="nt:unstructured"
                srcPath="/mnt/overlay/cq/core/content/header"/>
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/endor/layouts/content"/>
                <aside
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/endor/layouts/sidepanels"/>
                    <items jcr:primaryType="nt:unstructured">

                        <references
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container"
                            id="cq-rail-references">
                            <items jcr:primaryType="nt:unstructured">
                                <references
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/references/components/references"
                                    adminUrl="/sites.html"
                                    text="References">
                                    <items jcr:primaryType="nt:unstructured">
                                        <list
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/contsys"
                                            class="list">
                                            <items jcr:primaryType="nt:unstructured">
                                                <source
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/references/components/group"
                                                    title="Source">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <blueprint
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/references/components/type"
                                                            clientLibs="[cq.gui.common.admin.references.livecopy]"
                                                            itemResourceType="wcm/msm/components/references/blueprint"
                                                            title="Blueprint"
                                                            type="blueprint"/>
                                                    </items>
                                                </source>
                                                <copies
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/references/components/group"
                                                    title="Copies">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <launches
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/references/components/type"
                                                            clientLibs="[cq.gui.common.admin.references.launches]"
                                                            itemResourceType="launches/components/references/launchcopy"
                                                            title="Launches"
                                                            type="launch"/>
                                                        <liveCopies
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/references/components/type"
                                                            clientLibs="[cq.gui.common.admin.references.livecopy]"
                                                            itemResourceType="wcm/msm/components/references/livecopy"
                                                            title="Live Copies"
                                                            type="liveCopy"/>
                                                        <languageCopies
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/references/components/type"
                                                            clientLibs="[cq.gui.common.admin.references.languagecopy]"
                                                            itemResourceType="wcm/core/components/references/languagecopy"
                                                            multiSelect="true"
                                                            title="Language Copies"
                                                            type="languageCopy">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <start
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/container"
                                                                    class="detail-toolbar active start"
                                                                    projectDetails="/projects/details.html"
                                                                    translationProjectWorkflowModel="/etc/workflow/models/wcm-translation/prepare_translation_project/jcr:content/model"
                                                                    translationWorkflowModel="/etc/workflow/models/wcm-translation/translate-language-copy/jcr:content/model"
                                                                    workflowModel="/etc/workflow/models/wcm-translation/create_language_copy/jcr:content/model">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <trigger
                                                                            jcr:primaryType="nt:unstructured"
                                                                            jcr:title="Create &amp; Translate"
                                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                                            <layout
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible"
                                                                                collapsedIcon="icon-chevron-up"
                                                                                expandedIcon="icon-chevron-down"
                                                                                variant="block"/>
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <form
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/foundation/form">
                                                                                    <layout
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/foundation/layouts/form"/>
                                                                                    <items jcr:primaryType="nt:unstructured">
                                                                                        <languages
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                                                            class="languages"
                                                                                            emptyText="Select languages"
                                                                                            fieldLabel="Languages"
                                                                                            multiple="{Boolean}true"
                                                                                            name="languages"
                                                                                            translateOptions="{Boolean}true">
                                                                                            <datasource
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                sling:resourceType="wcm/core/components/references/languagecopy/datasources/languageroots"/>
                                                                                        </languages>
                                                                                        <deep
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                                                            checked="{Boolean}true"
                                                                                            deleteHint="{Boolean}false"
                                                                                            name="deep"
                                                                                            text="Select all sub-pages"
                                                                                            value="true"/>
                                                                                        <projectType
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                                                            class="projectType"
                                                                                            emptyText="Select a project"
                                                                                            fieldLabel="Project"
                                                                                            name="projectType">
                                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                                <new
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/option"
                                                                                                    text="Create a new translation project"
                                                                                                    value="add_new"/>
                                                                                                <create_structure
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/option"
                                                                                                    text="Create structure only"
                                                                                                    value="add_structure_only"/>
                                                                                                <existing
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/option"
                                                                                                    text="Add to existing translation project"
                                                                                                    value="add_existing"/>
                                                                                            </items>
                                                                                        </projectType>
                                                                                        <projectTitle
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/container"
                                                                                            class="projectTitle"
                                                                                            hidden="{Boolean}false">
                                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                                <title
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                                                    fieldLabel="Title"
                                                                                                    name="projectTitle"/>
                                                                                                <tooltip
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/text"
                                                                                                    class="fieldDescription"
                                                                                                    text="Multiple translation projects will be created"/>
                                                                                            </items>
                                                                                        </projectTitle>
                                                                                        <project
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                                                            emptyText="Select a project"
                                                                                            fieldLabel="Existing translation project"
                                                                                            name="project"
                                                                                            renderHidden="{Boolean}true">
                                                                                            <datasource
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                sling:resourceType="cq/gui/components/projects/admin/datasource/projectselectdatasource"
                                                                                                projectTemplates="[/libs/cq/core/content/projects/templates/create-translation-project]"/>
                                                                                        </project>
                                                                                        <create
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/button"
                                                                                            disabled="{Boolean}true"
                                                                                            oppositeText="Add"
                                                                                            role="submit"
                                                                                            text="Create"
                                                                                            variant="[primary,block]"/>
                                                                                    </items>
                                                                                </form>
                                                                            </items>
                                                                        </trigger>
                                                                    </items>
                                                                </start>
                                                                <update
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/foundation/container"
                                                                    class="detail-toolbar hidden update"
                                                                    workflowModel="/etc/workflow/models/wcm-translation/update_language_copy/jcr:content/model">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <trigger
                                                                            jcr:primaryType="nt:unstructured"
                                                                            jcr:title="Update language copies"
                                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                                            <layout
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible"
                                                                                collapsedIcon="icon-chevron-up"
                                                                                expandedIcon="icon-chevron-down"
                                                                                variant="block"/>
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <form
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/foundation/form">
                                                                                    <layout
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        sling:resourceType="granite/ui/components/foundation/layouts/form"/>
                                                                                    <items jcr:primaryType="nt:unstructured">
                                                                                        <deep
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/include"
                                                                                            path="wcm/core/content/sites/jcr:content/body/content/aside/items/references/items/references/items/list/items/copies/items/languageCopies/items/start/items/trigger/items/form/items/deep"/>
                                                                                        <projectType
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                                                            class="projectType"
                                                                                            emptyText="Select a project"
                                                                                            fieldLabel="Project"
                                                                                            name="projectType">
                                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                                <create_structure
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/option"
                                                                                                    text="Create structure only"
                                                                                                    value="add_structure_only"/>
                                                                                                <new
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/option"
                                                                                                    text="Create a new translation project"
                                                                                                    value="add_new"/>
                                                                                                <existing
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/option"
                                                                                                    text="Add to existing translation project"
                                                                                                    value="add_existing"/>
                                                                                            </items>
                                                                                        </projectType>
                                                                                        <projectTitle
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/container"
                                                                                            class="projectTitle"
                                                                                            hidden="{Boolean}true">
                                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                                <title
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                                                                    fieldLabel="Title"
                                                                                                    name="projectTitle"/>
                                                                                                <tooltip
                                                                                                    jcr:primaryType="nt:unstructured"
                                                                                                    sling:resourceType="granite/ui/components/foundation/text"
                                                                                                    class="fieldDescription"
                                                                                                    text="Multiple translation projects will be created"/>
                                                                                            </items>
                                                                                        </projectTitle>
                                                                                        <project
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                                                            emptyText="Select a project"
                                                                                            fieldLabel="Existing translation project"
                                                                                            name="project"
                                                                                            renderHidden="{Boolean}true">
                                                                                            <datasource
                                                                                                jcr:primaryType="nt:unstructured"
                                                                                                sling:resourceType="cq/gui/components/projects/admin/datasource/projectselectdatasource"
                                                                                                projectTemplates="[/libs/cq/core/content/projects/templates/create-translation-project]"/>
                                                                                        </project>
                                                                                        <start
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/foundation/button"
                                                                                            disabled="{Boolean}true"
                                                                                            role="submit"
                                                                                            text="Start"
                                                                                            variant="[primary,block]"/>
                                                                                    </items>
                                                                                </form>
                                                                            </items>
                                                                        </trigger>
                                                                    </items>
                                                                </update>
                                                            </items>
                                                        </languageCopies>
                                                    </items>
                                                </copies>
                                                <links
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/references/components/group"
                                                    title="Links">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <external
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/references/components/type"
                                                            clientLibs="[cq.gui.common.admin.references.externallink]"
                                                            itemResourceType="wcm/core/components/references/externallink"
                                                            title="External Links"
                                                            type="externalLink"/>
                                                    </items>
                                                </links>
                                                <contents
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/references/components/group"
                                                    title="Content References">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <borrowed
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/references/components/type"
                                                            clientLibs="[cq.gui.common.admin.references.borrowedcontent]"
                                                            itemResourceType="wcm/core/components/references/borrowedcontent"
                                                            title="Borrowed Content"
                                                            type="borrowedContent"/>
                                                        <lent
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/references/components/type"
                                                            clientLibs="[cq.gui.common.admin.references.shortenedlinks]"
                                                            itemResourceType="wcm/core/components/references/lentcontent"
                                                            title="Lent Content"
                                                            type="lentContent"/>
                                                    </items>
                                                </contents>
                                            </items>
                                        </list>
                                    </items>
                                </references>
                            </items>
                        </references>
                    </items>
                </aside>
                <header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/mode"
                        group="cq-siteadmin-admin-childpages"
                        mode="${state[&quot;cq.sites.childpages.selectionMode&quot;].boolean ? &quot;selection&quot; : &quot;default&quot; }"/>
                    <items jcr:primaryType="nt:unstructured">

                    </items>
                </header>
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/container">
                    <items jcr:primaryType="nt:unstructured">
                        <childpages
                            granite:rel="cq-siteadmin-admin-childpages"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container"
                            itemscope="{Boolean}true">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                foundation-mode-group="cq-siteadmin-admin-childpages"/>
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/multilayouts"
                                layout="${state[&quot;cq.sites.childpages.layoutId&quot;].string}"
                                skipEmpty="columns">
                                <layouts jcr:primaryType="nt:unstructured">
                                    <card
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/cardview"
                                        itemprop="item"
                                        layoutId="card"
                                        limit="{Long}20"
                                        maximized="{Boolean}true"
                                        path="/libs/harbor/admin/documentation/components"
                                        selectionMode="${state[&quot;cq.sites.childpages.selectionMode&quot;].boolean}"
                                        src="/libs/wcm/core/content/sites/jcr:content/body/content/content/items/childpages{.offset,limit}.html{+id}"
                                        stateId="cq.sites.childpages"/>
                                    <list
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/listview"
                                        itemprop="item"
                                        layoutId="list"
                                        limit="{Long}40"
                                        maximized="{Boolean}true"
                                        path="/libs/harbor/admin/documentation/components"
                                        src="/libs/wcm/core/content/sites/jcr:content/body/content/content/items/childpages{.offset,limit}.html{+id}"
                                        stateId="cq.sites.childpages">
                                        <headerdatasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/siteadmin/admin/listview/columns/datasources/columndatasource"
                                            class="card-page"
                                            moveable="{Boolean}true"
                                            selectable="{Boolean}true"/>
                                    </list>
                                    <columns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/columnview"
                                        itemprop="item"
                                        itemResourceType="cq/gui/components/siteadmin/admin/pagecolumnitem"
                                        layoutId="columns"
                                        limit="{Long}40"
                                        loadParents="{Boolean}true"
                                        multiselect="{Boolean}true"
                                        path="/libs/harbor/admin/documentation/components"
                                        rootPath="/libs/harbor/admin/documentation/components"
                                        src="/libs/wcm/core/content/sites/column{.offset,limit}.html{+path}"
                                        stateId="cq.sites.childpages">
                                        <preview
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/siteadmin/admin/pagecolumnpreview"
                                            path="/libs/harbor/admin/documentation/components"/>
                                    </columns>
                                </layouts>
                                <empty
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/common/wcm/layouts/emptychildpages"
                                    path="/libs/harbor/admin/documentation/components">

                                    <actions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/endor/actionbar/hyperlink"
                                        hideText="{Boolean}true"
                                        href="#empty-actions-popover"
                                        icon="icon-add"
                                        text="Create"
                                        title="Create"
                                        toggle="popover"/>
                                </empty>
                            </layout>
                            <datasource
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/common/wcm/datasources/childpages"
                                exclude="/content/(catalogs|dam|launches|mac|publications|usergenerated|communities|community-components|community-templates|forms|projects|phonegap)(/.*)*"
                                itemResourceType="cq/gui/components/siteadmin/admin/pagecard"
                                limit="${empty requestPathInfo.selectors[1] ? &quot;20&quot; : requestPathInfo.selectors[1]}"
                                offset="${requestPathInfo.selectors[0]}"
                                path="/libs/harbor/admin/documentation/components"/>
                        </childpages>

                    </items>
                </content>
            </content>
        </body>
    </jcr:content>
    <properties/>
    <folderproperties/>
    <createpagewizard/>
    <createsitewizard/>
    <createlaunchwizard/>
    <createlivecopywizard/>
    <movepagewizard/>
    <publishpagewizard/>
    <unpublishpagewizard/>
    <promotelaunchwizard/>
    <editlivecopywizard/>
    <column
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/columnview"
            itemResourceType="cq/gui/components/siteadmin/admin/pagecolumnitem"
            path="/libs/harbor/admin/documentation/components"
            src="/libs/wcm/core/content/sites/column{.offset,limit}.html{+path}"/>
        <datasource
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/gui/components/common/wcm/datasources/childpages"
            exclude="/content/(catalogs|dam|launches|mac|publications|usergenerated|communities|community-components|community-templates|forms|projects|phonegap)(/.*)*"
            itemResourceType="cq/gui/components/siteadmin/admin/pagecard"
            limit="${empty requestPathInfo.selectors[1] ? &quot;40&quot; : requestPathInfo.selectors[1]}"
            offset="${requestPathInfo.selectors[0]}"
            path="/libs/harbor/admin/documentation/components"/>
    </column>
    <previewcolumn
        jcr:primaryType="nt:unstructured"
        sling:resourceType="cq/gui/components/siteadmin/admin/pagecolumnpreview"
        path="/libs/harbor/admin/documentation/components"/>
</jcr:root>
