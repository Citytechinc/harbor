                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--3"
                                    level="3"
                                    text="com.icfolson.aem.harbor.api.lists.construction.ListConstructionStrategy&lt;T&gt;"/>
                            <listconstructionstrategy-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="screens/ui/components/admin/text"
                                    line1="Represents an implementation of a mechanism for the building up of a list of items of type T."
                                    />
                            <listconstructionstrategy-method-table
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="harbor/admin/documentation/table">
                                <header1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Method Signature"
                                        />
                                <header2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Description"
                                        />
                                <row1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                    <row1col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public Iterable&lt;T&gt; construct"
                                            />
                                    <row1col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="Produces a list of items.  Implementations should focus on the construction of a suitable item list and should not include in the business logic of the method any consideration of the list's presentation as this is the responsibility of the rendering strategy."
                                            />
                                </row1>
                            </listconstructionstrategy-method-table>


                            <listrenderingstrategy-title
