                        <items jcr:primaryType="nt:unstructured">
                            <!-- Start of List Documentation -->
                            <list-section-heading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--2"
                                    level="2"
                                    text="Lists"/>
                            <description-p1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="screens/ui/components/admin/text"
                                    line1="The functionality of many of the Components created for a particular project can be distilled down to the presentation of a List of items.  Lists therefore are central to Harbor and enabled by Interfaces and Abstract Classes suggesting a pattern to follow in List Component development."
                                    />
                            <description-p2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="screens/ui/components/admin/text"
                                    line1="List Components are a composition of a List Construction Strategy and a List Rendering Strategy.  The List Construction Strategy is responsible for the creation of a list of items based on the logic of the strategy.  These items are passed to the List Rendering Strategy which has an opportunity to add additional properties germane to the rendering of a particular component.  Construction and Rendering Strategies may be mixed and matched within the context of concrete List Component implementations allowing reuse of both across multiple component types."
                                    />
                            <description-p3
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="screens/ui/components/admin/text"
                                    line1="TODO: Insert example of reuse from the various page listing components"
                                    />

                            <listcomponent-title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--3"
                                    level="3"
                                    text="com.icfolson.aem.harbor.api.components.content.list.ListComponent&lt;T extends Iterable&gt;"/>
                            <listcomponent-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="screens/ui/components/admin/text"
                                    line1="A ListComponent is a component type which intends to hold and manage an ordered collection of items for rendering."
                                    />
                            <listcomponent-method-table
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="harbor/admin/documentation/table">
                                <header1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Method Signature"
                                        />
                                <header2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Description"
                                        />
                                <row1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                    <row1col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public T getItems"
                                            />
                                    <row1col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="Returns the Iterable containing the list items along with any properties necessary for rendering of the component as a whole."
                                            />
                                </row1>
                                <row2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                    <row2col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public Iterator&lt;?&gt; getIterator"
                                            />
                                    <row2col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="Provides an Iterator for the Iterable returned by getItems.  This method exists largely to service jsp component implementations where the c:forEach loop does not accept Iterables.  If using Sightly this method should not need to be used."
                                            />
                                </row2>
                            </listcomponent-method-table>


                            <listconstructionstrategy-title
