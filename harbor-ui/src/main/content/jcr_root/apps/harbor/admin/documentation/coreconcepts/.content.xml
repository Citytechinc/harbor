<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Core Concepts"
        sling:resourceType="granite/ui/components/foundation/page"
        sling:vanityOrder="{Long}301"
        sling:vanityPath="">

        <head jcr:primaryType="nt:unstructured">
            <viewport
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/meta"
                content="chrome=1"
                name="X-UA-Compatible"/>
            <winmode
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/page/winmode"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/page/title"/>
            <favicon
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/page/favicon"/>
            <clientlibs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                categories="[coralui2,granite.ui.foundation,granite.ui.foundation.admin,cq.common.wcm,cq.siteadmin.admin,cq.listview.columns.personalization,cq.listview.analytics,cq.siteadmin.admin.pagecard]"/>
        </head>
        <body
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/page/body">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/layouts/page"/>
            <rail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/endor/navcolumns"
                currentId="harbor-admin-documentation-coreconcepts"
                srcPath="cq/core/content/nav"/>
            <breadcrumbs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="harbor/admin/documentation/breadcrumb"
                    href="/apps/harbor/admin/documentation.html"
                    title="Harbor Documentation"/>
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/siteadmin/admin/endor/title"
                contentPath="/apps/harbor/admin/documentation/coreconcepts"
                rootTitle="Harbor"/>
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                        type="large"/>
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container"
                            jcr:title="Author Guide">
                        <items jcr:primaryType="nt:unstructured">
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--1"
                                    level="2"
                                    text="Core Concepts"/>
                            <description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/text"
                                    text="Core Concepts."/>
                        </items>
                    </tab1>
                    <tab2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container"
                            jcr:title="Developer Guide">
                        <items jcr:primaryType="nt:unstructured">
                            <list-section-heading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--2"
                                    level="2"
                                    text="Lists"/>
                            <description-p1
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/text"
                                    text="The functionality of many of the Components created for a particular project can be distilled down to the presentation of a List of items.  Lists therefore are central to Harbor and enabled by Interfaces and Abstract Classes suggesting a pattern to follow in List Component development."
                                    />
                            <description-p2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/text"
                                    text="List Components are a composition of a List Construction Strategy and a List Rendering Strategy.  The List Construction Strategy is responsible for the creation of a list of item based on the logic of the strategy.  These items are passed to the List Rendering Strategy which has an opportunity to add additional properties germane to the rendering of a particular component.  Construction and Rendering Strategies may be mixed and matched within the context of concrete List Component implementations allowing reuse of both across multiple component types."
                                    />


                            <listcomponent-title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--3"
                                    level="3"
                                    text="com.icfolson.aem.harbor.api.components.content.list.ListComponent&lt;T extends Iterable&gt;"/>
                            <listcomponent-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/text"
                                    text="A ListComponent is a component type which intends to hold and manage an ordered collection of components."
                                    />
                            <listcomponent-method-table
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="harbor/admin/documentation/table">
                                <header1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Method Signature"
                                        />
                                <header2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Description"
                                        />
                                <row1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                    <row1col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public T getItems"
                                            />
                                    <row1col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text=""
                                            />
                                </row1>
                                <row2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                    <row2col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public Iterator&lt;?&gt; getIterator"
                                            />
                                    <row2col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="Provides an Iterator for the Iterable returned by getItems.  This method exists largely to service jsp component implementations where the c:forEach loop does not accept Iterables.  If using Sightly this method should not need to be used."
                                            />
                                </row2>
                            </listcomponent-method-table>


                            <listconstructionstrategy-title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--3"
                                    level="3"
                                    text="com.icfolson.aem.harbor.api.lists.construction.ListConstructionStrategy&lt;T&gt;"/>
                            <listconstructionstrategy-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/text"
                                    text="Represents an implementation of a mechanism for the building up of a list of items of type T."
                                    />
                            <listconstructionstrategy-method-table
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="harbor/admin/documentation/table">
                                <header1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Method Signature"
                                        />
                                <header2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Description"
                                        />
                                <row1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                    <row1col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public Iterable&lt;T&gt; construct"
                                            />
                                    <row1col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text=""
                                            />
                                </row1>
                            </listconstructionstrategy-method-table>


                            <listrenderingstrategy-title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--3"
                                    level="3"
                                    text="com.icfolson.aem.harbor.api.lists.rendering.ListRenderingStrategy&lt;T, R extends Iterable&lt;?&gt;&gt;"/>
                            <listrenderingstrategy-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/text"
                                    text="A List Rendering Strategy is able to take an Iterable of type T and produce a new Iterable imbued with any properties which are germane specifically to the rendering of the items in the list."
                                    />
                            <listrenderingstrategy-method-table
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="harbor/admin/documentation/table">
                                <header1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Method Signature"
                                        />
                                <header2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/heading"
                                        text="Description"
                                        />
                                <row1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                    <row1col1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public R toRenderableList(Iterable&lt;T&gt;)"
                                            />
                                    <row1col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text=""
                                            />
                                </row1>
                            </listrenderingstrategy-method-table>

                        </items>
                    </tab2>

                </items>
            </content>
        </body>
    </jcr:content>
</jcr:root>
