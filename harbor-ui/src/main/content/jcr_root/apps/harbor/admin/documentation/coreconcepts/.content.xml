                                    text="com.icfolson.aem.harbor.api.lists.rendering.ListRenderingStrategy&lt;T, R extends Iterable&lt;?&gt;&gt;"/>
                            <listrenderingstrategy-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="screens/ui/components/admin/text"
                                    line1="A List Rendering Strategy is able to take an Iterable of type T and produce a new Iterable imbued with any properties which are germane specifically to the rendering of the items in the list."
                                    />
                            <listrenderingstrategy-method-table
                                    jcr:primaryType="nt:unstructured"
