                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="public R toRenderableList(Iterable&lt;T&gt;)"
                                            />
                                    <row1col2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/text"
                                            text="Transforms a list of items into an Iterable suitable for rendering."
                                            />
                                </row1>
                            </listrenderingstrategy-method-table>


                            <abstractlistcomponent-title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/heading"
                                    class="coral-Heading coral-Heading--3"
                                    level="3"
                                    text="com.icfolson.aem.harbor.core.components.content.list.AbstractListComponent&lt;T, R extends Iterable&lt;?&gt;&gt;"/>
                            <abstractlistcomponent-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="screens/ui/components/admin/text"
                                    line1="The AbstractListComponent class embodies the common pattern for the usage of ListConstructionStrategies and ListRenderingStrategies within the context of a concrete list component within its implementation of the getItems method."
                                    />
